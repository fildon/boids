{
    "name": "boids",
    "description": "An implementation of boids",
    "repository": {
        "type": "git",
        "url": "https://github.com/fildon/fildon.github.io"
    },
    "license": "MIT",
    "version": "1.0.0",
    "main": "app.js",
    "scripts": {
        "build": "yarn webpack --config webpack.config.js --mode=production",
        "lint": "tslint -c tslint.json src/**/*.ts",
        "mutation": "stryker run",
        "test": "nyc mocha",
        "coverage": "nyc --check-coverage mocha"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "html"
        ],
        "all": true
    },
    "dependencies": {},
    "devDependencies": {
        "@stryker-mutator/core": "^1.3.1",
        "@stryker-mutator/mocha-framework": "^1.3.1",
        "@stryker-mutator/mocha-runner": "^1.3.1",
        "@stryker-mutator/typescript": "^1.3.1",
        "@types/chai": "^4.1.2",
        "@types/mocha": "^2.2.48",
        "@types/node": "^9.4.5",
        "@types/sinon": "^5.0.2",
        "chai": "^4.1.2",
        "minimist": ">=1.2.3",
        "mocha": "^5.0.0",
        "sinon": "^6.3.5",
        "ts-loader": "^7.0.5",
        "ts-node": "^4.1.0",
        "tslint": "^5.9.1",
        "typescript": "^3.9.3",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11"
    }
}
